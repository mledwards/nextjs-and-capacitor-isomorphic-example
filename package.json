  "dependencies": {
    "@auth0/auth0-react": "^1.8.0",
    "@capacitor/android": "^3.2.4",
    "@capacitor/app": "^1.0.3",
    "@capacitor/browser": "^1.0.3",
    "@capacitor/cli": "^3.2.4",
    "@capacitor/core": "^3.2.4",
    "cors": "^2.8.5",
    "next": "^11.1.2",
