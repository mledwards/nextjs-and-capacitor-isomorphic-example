{
  "name": "nextjs-and-capacitor-isomorphic-example",
  "version": "1.0.0",
  "description": "Example of using next.js (client side and server side) with capacitor",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "build-mobile": "next build && next export && npx cap sync",
    "build-mobile-run-web": "next build && next export && npx cap sync && npm run start",
    "build-mobile-run-android": "next build && next export && npx cap sync && npx cap open android",
    "server-dev": "cd server && next dev -p 3001",
    "client-dev": "next dev -p 3000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mledwards/nextjs-and-capacitor-isomorphic-example.git"
